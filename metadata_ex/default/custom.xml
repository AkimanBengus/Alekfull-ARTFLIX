<theme>
	<formatVersion>4</formatVersion>

	<view name="system">
		<text name="mdex_systemName" extra="true">
			<visible>true</visible>
		</text>
		
		<text name="mdex_systemCreatorType" extra="true">
			<visible>false</visible>
		</text>

		<text name="mdex_systemYear" extra="true">
			<visible>false</visible>
		</text>

		<text name="mdex_systemCreatorType" extra="true">
			<visible>false</visible>
		</text>

		<text name="mdex_systemCreator" extra="true">
			<visible>false</visible>
		</text>

		<text name="mdex_systemDescription" extra="true">
			<visible>false</visible>
		</text>

	</view>

	<customView name="HCoinSpin">
		<image name="overlayCoinOps" extra="true">
			<path>./assets/coinops/default.png</path>
			<zIndex>7</zIndex>
		</image>
	</customView>


	<customView name="CoinOps Details">
		<image name="overlayCoinOps" extra="true">
			<path>./assets/coinops/default.png</path>
			<zIndex>7</zIndex>
		</image>
	</customView>

</theme>
