<?xml version="1.0" encoding="UTF-8"?>
<theme defaultView="CoinOps Details">
	<formatVersion>7</formatVersion>
	<include>./systemview/systemview.xml</include>
	<include>./metadata_ex/$system/custom.xml</include>

	<!-- To Translate -->
	<include subset="screenType" name="Full HD 1080p">./systemview/screenTypeFullHD.xml</include>
	<include subset="screenType" name="HD 720p">./systemview/screenTypeHD.xml</include>
	<include subset="screenType" name="Lite 540p">./systemview/screenTypeLite.xml</include>

	<include subset="carouselStyle" name="Horizontal" displayName="Horizontal">./systemview/systemview_carousel_horizontal.xml</include>
	<include subset="carouselStyle" name="Vertical" displayName="Vertical">./systemview/systemview_carousel_vertical.xml</include>
	<include subset="carouselStyle" name="Vertical Wheel" displayName="Vertical Wheel">./systemview/systemview_carousel_vertical_wheel.xml</include>
	
	<include subset="headerStyle" name="System Logo" displayName="System Logo">./systemview/systemview_header_logo.xml</include>
	<include subset="headerStyle" name="System Name" displayName="System Name">./systemview/systemview_header_text.xml</include>

	<include subset="music" name="Mute" displayName="${silence}">./nosounds.xml</include>
	<include subset="music" name="On" displayName="${sysmusic}">./sounds.xml</include>
	<include subset="music" name="Off" displayName="${nomusic}"/>

	<include subset="mainBackgroundColor" name="Gray" displayName="${gray}"/>
	<include subset="mainBackgroundColor" name="Green" displayName="${green}">./background_colors/green.xml</include>
	<include subset="mainBackgroundColor" name="Red" displayName="${red}">./background_colors/red.xml</include>
	<include subset="mainBackgroundColor" name="Blue" displayName="${blue}">./background_colors/blue.xml</include>
	<include subset="mainBackgroundColor" name="Purple" displayName="${purple}">./background_colors/purple.xml</include>

	<include subset="shader" name="None" displayName="${none}"/>
	<include subset="shader" name="CRT">./effects/crtSystem.xml</include>
	<include subset="shader" name="CRT Full Screen" displayName="${shaderCrtFullScreen}">./effects/crtScreen.xml</include>
	<include subset="shader" name="Phosphore">./effects/phosphore.xml</include>
	<include subset="shader" name="Phosphore +">./effects/phosplus.xml</include>
	<include subset="shader" name="ScanLines">./effects/scanlinesSystem.xml</include>
	<include subset="shader" name="ScanLines Full Screen" displayName="${shaderScanlinesFullScreen}">./effects/scanlinesScreen.xml</include>
	<include subset="shader" name="Scanlines Light" displayName="${shaderScanlinesLight}">./effects/scanlinesLight.xml</include>
	<include subset="shader" name="Scanlines Vertical">./effects/scanlinesVertical.xml</include>
	<include subset="shader" name="Scanlines Vertical Light">./effects/scanlinesVerticalLight.xml</include>
	<include subset="shader" name="CRT Glitch (Heavy)" displayName="${shaderCrtGlitch}">./effects/crtvideo.xml</include>
	<include subset="shader" name="VHS Glitch (Heavy)" displayName="${shaderVhsGlich}">./effects/vhs.xml</include>

	<include subset="tveffect" name="None" displayName="${none}" appliesTo="HCoinSpin, CoinOps Details"/>
	<include subset="tveffect" name="Glitch" appliesTo="HCoinSpin, CoinOps Details">./effects/vhsCoin.xml</include>
	<include subset="tveffect" name="CRT" appliesTo="HCoinSpin, CoinOps Details">./effects/crtCoin.xml</include>

	<include subset="showTopMarquee" name="Yes" displayName="${yes}" appliesTo="HCoinSpin, CoinOps Details">./view_options/wmarquee.xml</include>
	<include subset="showTopMarquee" name="No" displayName="${no}" appliesTo="HCoinSpin, CoinOps Details">./view_options/nmarquee.xml</include>

	<include subset="coinOpsDefault" name="Default" displayName="${default}" appliesTo="HCoinSpin, CoinOps Details">./view_options/coinOps.xml</include>
	<include subset="coinOpsDefault" name="TV Only" displayName="${coinOpsTV}" appliesTo="HCoinSpin, CoinOps Details">./view_options/coinOpsTV.xml</include>
	<include subset="coinOpsDefault" name="Arcade Only" displayName="${coinOpsArcade}" appliesTo="HCoinSpin, CoinOps Details">./view_options/coinOpsArcade.xml</include>

	<!-- Subset options for color  -->
	<include subset="colorset" name="Red" displayName="${red}"/>
	<include subset="colorset" name="Blue" displayName="${blue}">./menu/colors/blue.xml</include>
	<include subset="colorset" name="Green" displayName="${green}">./menu/colors/green.xml</include>
	<include subset="colorset" name="Cyan">./menu/colors/cyan.xml</include>
	<include subset="colorset" name="Yellow" displayName="${yellow}">./menu/colors/yellow.xml</include>
	<include subset="colorset" name="Violet">./menu/colors/violet.xml</include>
	<include subset="colorset" name="Orange">./menu/colors/orange.xml</include>
	<include subset="colorset" name="Gray" displayName="${gray}">./menu/colors/gray.xml</include>

	<variables>
		<subset.mainBackgroundColor>Main Background Color</subset.mainBackgroundColor>
		<subset.coinOpsDefault>CoinOps Default System</subset.coinOpsDefault>
		<subset.tveffect>TV Effect CoinOps View</subset.tveffect>
