	<feature supported="carousel">
		<view name="system">
			<sound name="bgsound">
			</sound>
		</view>
	</feature>

	<view name="menu">
		<menuText name="menutitle">
			<fontPath>./fonts/Cabin-Bold.ttf</fontPath>
			<fontSize>0.044</fontSize>
			<color>FAFAFA</color>
		</menuText>
		<menuText name="menufooter">
			<fontPath>./fonts/Cabin-Regular.ttf</fontPath>
			<fontSize>0.02</fontSize>
			<color>777777FF</color>
		</menuText>
		<menuText name="menutext">
			<fontPath>./fonts/Cabin-Regular.ttf</fontPath>
			<fontSize>0.0325</fontSize>
			<fontSize tinyScreen="true">0.034</fontSize>
			<color>969696</color>
			<separatorColor>282828</separatorColor>
			<selectorColor>${baseColor}</selectorColor>
			<selectorColorEnd>${gradientEndColor}</selectorColorEnd>
			<selectedColor>FFFFFF</selectedColor>
			<selectorGradientType>horizontal</selectorGradientType>
		</menuText>
		<menuTextSmall name="menutextsmall">
			<fontPath>./fonts/Cabin-Regular.ttf</fontPath>
			<fontSize>0.02</fontSize>
			<color>${groupColor}</color>
		</menuTextSmall>
		<menuGroup name="menugroup">
			<fontPath>./fonts/Cabin-Bold.ttf</fontPath>
			<fontSize>0.025</fontSize>
			<color>${groupColor}</color>
			<separatorColor>${groupSeparatorColor}</separatorColor>
			<backgroundColor>00000000</backgroundColor>
			<lineSpacing>2.5</lineSpacing>
		</menuGroup>
		<menuBackground name="menubg">
			<color>242424</color>
			<cornerSize>32 32</cornerSize>
			<path>./menu/frame.png</path>
			<scrollbarColor>96969620</scrollbarColor>
			<scrollbarCorner>0.49</scrollbarCorner>
			<scrollbarSize>0.003</scrollbarSize>
			<scrollbarAlignment>innerright</scrollbarAlignment>
		</menuBackground>
		<menuIcons name="menuicons">
			<iconKodi>./icons/Kodi.png</iconKodi>
			<iconSystem>./icons/System.png</iconSystem>
			<iconUpdates>./icons/Updates.png</iconUpdates>
			<iconGames>./icons/Games.png</iconGames>
			<iconControllers>./icons/Controllers.png</iconControllers>
			<iconUI>./icons/UI.png</iconUI>
			<iconSound>./icons/Sound.png</iconSound>
			<iconNetwork>./icons/Network.png</iconNetwork>
			<iconScraper>./icons/Scraper.png</iconScraper>
			<iconAdvanced>./icons/Advanced.png</iconAdvanced>
			<iconQuit>./icons/Quit.png</iconQuit>
			<iconRestart>./icons/Restart.png</iconRestart>
			<iconShutdown>./icons/Shutdown.png</iconShutdown>
			<iconFastShutdown>./icons/Fastshutdown.png</iconFastShutdown>
			<iconRetroachievements>./icons/Contest.png</iconRetroachievements>
			<iconManual>./icons/Manual.png</iconManual>
		</menuIcons>
		<menuTextEdit name="menutextedit">
			<inactive>./menu/textinput_ninepatch.png</inactive>
			<active>./menu/textinput_ninepatch_active.png</active>
		</menuTextEdit>
		<menuButton name="menubutton">
			<path>./menu/button.png</path>
			<filledPath>./menu/button_filled.png</filledPath>
			<cornerSize>8 8</cornerSize>
		</menuButton>
	</view>

	<view name="screen">
		<controllerActivity name="controllerActivity">
			<pos>0.015 0.02</pos>
			<size>0.085 0.025</size>
			<itemSpacing>0.003</itemSpacing>
			<imagePath>./assets/gamepad.svg</imagePath>
			<color>FFFFFF80</color>
			<activityColor>A0A0FFC0</activityColor>
			<hotkeyColor>FFA0A0C0</hotkeyColor>
		</controllerActivity>
	</view>

	<view name="menu, system, grid">
		<helpsystem name="help">
			<textColor>FFFFFF</textColor>
			<iconColor>7d7d7d</iconColor>
			<fontSize>0.030</fontSize>
			<pos>0.02 0.96</pos>

			<pos verticalScreen="true">0.5 0.96</pos>
			<origin verticalScreen="true">0.5 0.5</origin>
			<fontSize verticalScreen="true">0.025</fontSize>
		</helpsystem>
	</view>

	<view name="grid">
		<image name="logo">
			<!-- try png -->
			<path>./../assets/logos/$system.png</path>
			<!-- try svg -->
			<path>./../assets/logos/$system.svg</path>
			<!-- check if lang resources exist -->
			<path>./../assets/logos/${system.theme}-${lang}.png</path>
			<path>./../assets/logos/${system.theme}-${lang}.svg</path>
			<!--
			<pos>0.02 0.12</pos>
			<maxSize>0.34 0.16</maxSize>
			-->
			<pos>0.02 0.12</pos>
			<maxSize>0.34 0.16</maxSize>
			<origin>0 0</origin>
			<zIndex>4</zIndex>
		</image>

		<image name="backgroundSystem" extra="true">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<path>./assets/arts/default.jpg</path>
			<path>./assets/arts/$system.jpg</path>
			<zIndex>2</zIndex>
		</image>

		<image name="backgroundOverlay" extra="true">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<path>./assets/filter4.png</path>
			<zIndex>5</zIndex>
		</image>

		<text name="md_lbl_rating, md_lbl_releasedate, md_lbl_developer, md_lbl_publisher, md_lbl_genre, md_lbl_players, md_lbl_lastplayed, md_lbl_playcount">
			<color>A0A0B000</color>
			<fontPath>./fonts/Homizio.ttf</fontPath>
		</text>
		<text name="md_developer, md_publisher, md_genre, md_players, md_playcount, md_description">
			<color>ffffff00</color>
			<fontPath>./fonts/Homizio.ttf</fontPath>
		</text>
		<datetime name="md_releasedate, md_lastplayed">
			<color>ffffff00</color>
			<fontPath>./fonts/Homizio.ttf</fontPath>
		</datetime>
		<rating name="md_rating">
			<pos>0.88 0.88</pos>
			<size>0.05 0.03</size>
			<color>FFFFFF</color>
			<unfilledColor>808080</unfilledColor>
			<fontPath>./fonts/Homizio.ttf</fontPath>
		</rating>
		<imagegrid name="gamegrid">
			<pos>0.025 0.16</pos>
			<size>0.950 0.774</size>
			<margin>0.005 0.010</margin>
			<padding>0.03 0.05 0.03 0.04</padding>
			<autoLayout>5 2</autoLayout>
			<autoLayoutSelectedZoom>1.12</autoLayoutSelectedZoom>
			<animateSelection>false</animateSelection>
		</imagegrid>
		<gridtile name="default">
			<backgroundColor>FFFFFF00</backgroundColor>
			<backgroundCornerSize>9 9</backgroundCornerSize>
			<selectionMode>image</selectionMode>
			<padding>5 5</padding>
			<imageColor>FFFFFFB0</imageColor>
		</gridtile>
		<gridtile name="selected">
			<backgroundCenterColor>0b0102</backgroundCenterColor>
			<backgroundEdgeColor>f60915</backgroundEdgeColor>
			<imageColor>FFFFFFFF</imageColor>
		</gridtile>
		<text name="gridtile">
			<color>D0D0D0</color>
			<backgroundColor>00000000</backgroundColor>
			<fontSize>0.022</fontSize>
			<alignment>center</alignment>
			<size>1 0.24</size>
		</text>
		<text name="gridtile_selected">
			<color>FFFFFF</color>
			<glowColor>00000016</glowColor>
			<glowSize>3</glowSize>
		</text>

		<text name="md_name">
			<fontPath>./fonts/Homizio.ttf</fontPath>
		</text>
	</view>

	<customView name="Artflix (Default)" inherits="grid" tinyScreen="false" displayName="${hartfdefViewName}">
		<text name="md_name">
			<color>ffffffff</color>
			<pos>0.025 0.16</pos>
			<size>0.92 0.10</size>
			<alignment>left</alignment>
			<glowColor>00000016</glowColor>
			<glowSize>4</glowSize>
			<fontPath>./fonts/Homizio.ttf</fontPath>
			<zIndex>0</zIndex>
		</text>

		<imagegrid name="gamegrid">
			<pos>0.72 0</pos>
			<size>0.3 1</size>
			<scrollDirection>vertical</scrollDirection>
			<autoLayout>1 7</autoLayout>
			<autoLayoutSelectedZoom>1.30</autoLayoutSelectedZoom>
			<margin>0.005 0.010</margin>
			<centerSelection>true</centerSelection>
			<scrollLoop>true</scrollLoop>
			<imageSource>marquee</imageSource>
		</imagegrid>

		<text name="md_developer">
			<fontPath>./assets/Circular Std Book.ttf</fontPath>
			<color>5c5b5c</color>
			<alignment>left</alignment>
			<fontSize>0.023</fontSize>
			<lineSpacing>1</lineSpacing>
			<size>0.21 0.03</size>
			<pos>0.195 0.374</pos>
			<glowColor>00000016</glowColor>
			<glowSize>1</glowSize>
			<zIndex>42</zIndex>
		</text>

		<text name="md_lbl_developer">
			<fontPath>./assets/Circular Std Black.ttf</fontPath>
			<color>FFFFFF</color>
			<alignment>left</alignment>
